<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Express EJS</title>
    <script src="https://telegram.org/js/telegram-web-app.js"></script>
  </head>
  <body>
    <h1 class="title">Welcome to Express with EJS</h1>
    <script>
      // Buttons
      const MainButton = window.Telegram.WebApp.MainButton;
      const BackButton = window.Telegram.WebApp.BackButton;
      const MainTitile = document.querySelector(".title");
      const SettingsButton = window.Telegram.WebApp.SettingsButton;
      MainButton.setText("Buyurtma berish");
      MainButton.show();
      MainButton.onClick(() => {
        BackButton.onClick(() => {
          MainTitile.textContent = "Buyurtmalar";
        });
        BackButton.show();
      });
      SettingsButton.show();
      SettingsButton.onClick(() => {
        MainTitile.textContent = "Setting page";
      });
    </script>
  </body>
</html>
